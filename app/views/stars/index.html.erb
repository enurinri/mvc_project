<h1 class="display-4 text-primary">Stars#index</h1>
<p><%= @msg %></p>
<table class="table">
<tr>
<th>ID</th><th>Name</th><th>Edit/Delete</th>
</tr>
<% @data.each do |obj|%>
<tr>
<td><%= obj.id%></td>
<td><a href="/stars/<%= obj.id%>"><%= obj.name%></a></td>
<td><a href="/stars/edit/<%= obj.id %>">edit</a>&nbsp;&nbsp;
<a href="javascript:delData(<%= obj.id %>);">delete</a></td>
</tr>
<%end%>
</table>

<script>
function  delData(num) {
    if(confirm('Will you delete this data?')){
        document.location="/stars/delete/"+num;
    }
    return false;
}
</script>
